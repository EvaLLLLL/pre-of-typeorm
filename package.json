    "@types/ua-parser-js": "^0.7.36",
    "antd": "^4.20.7",
    "axios": "^0.27.2",
    "next": "12.1.6",
    "pg": "^8.7.3",
    "react": "18.1.0",
    "react-dom": "18.1.0",
    "reflect-metadata": "^0.1.13",
    "typeorm": "^0.2.45",
    "ua-parser-js": "^1.0.2"
  },
  "devDependencies": {
    "@babel/plugin-proposal-decorators": "^7.18.2",
    "@types/axios": "^0.14.0",
    "@types/node": "^17.0.36",
    "@types/react": "18.0.9",
    "@types/react-dom": "18.0.5",
    "eslint": "8.16.0",
    "eslint-config-next": "12.1.6",
