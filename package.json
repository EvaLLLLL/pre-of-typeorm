{
  "name": "pre-of-typeorm",
  "version": "0.1.0",
  "scripts": {
    "dev": "next dev & babel -w ./src --out-dir dist --extensions .ts,.tsx",
    "build": "next build",
    "start": "next start",
    "tpo:build": "rm -rf dist && babel ./src --out-dir dist --extensions .ts,.tsx",
    "seed": "node dist/seed.js",
